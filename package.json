    "highlightjs-solidity": "git+https://github.com/highlightjs/highlightjs-solidity.git",
    "moment": "^2.19.4",
    "rsk-utils": "git+https://github.com/rsksmart/rsk-utils.git",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.10",
    "vue-d3-barchart": "latest",
    "vue-gtag": "^1.1.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
