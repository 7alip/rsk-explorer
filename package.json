{
  "name": "rsk-explorer",
  "version": "1.0.4",
  "private": true,
  "description": "Rsk explorer client",
  "author": "emi <emiliorizzo@gmail.com>",
  "scripts": {
    "build": "npx vue-cli-service build --modern",
    "build-with-tracking": "npx vue-cli-service build src/main.with.tracking.js --modern",
    "test:unit": "npx vue-cli-service test:unit",
    "test:e2e": "npx vue-cli-service test:e2e",
    "lint": "npx vue-cli-service lint",
    "dev": "npx vue-cli-service serve",
    "dev-with-tracking": "npx vue-cli-service serve src/main.with.tracking.js",
    "svg": "npx vsvg -s ./public/svg/icons -t ./src/icons --es6"
  },
  "dependencies": {
    "bignumber.js": "^7.2.1",
